		WebDriver driver=new ChromeDriver();
		driver.manage().window().maximize();
		driver.get("https://demoqa.com/automation-practice-form");
		driver.findElement(By.id("firstName")).sendKeys("Nishchay");
		driver.findElement(By.id("lastName")).sendKeys("Gupta");
		driver.findElement(By.id("userEmail")).sendKeys("hellonishchay@gmail.com");
		driver.manage().timeouts().implicitlyWait(Duration.ofSeconds(5));
		
		//sending male via radio key
		List<WebElement> webElements = driver.findElements(By.cssSelector("label[class=\"custom-control-label\"]"));
		Actions actions = new Actions(driver);
		for(WebElement x : webElements) {
			if(x.getText().equalsIgnoreCase("male")) {
				actions.moveToElement(x).click().build().perform();
			}
		}
		driver.findElement(By.id("userNumber")).sendKeys("1234567890");
//		
		WebElement dobElement=driver.findElement(By.id("dateOfBirthInput"));
		((JavascriptExecutor) driver).executeScript("arguments[0].scrollIntoView(true);", dobElement);
		dobElement.click();
		WebElement dmElement = driver.findElement(By.className("react-datepicker__month-select"));
		Select dmSelect = new Select(dmElement);
		dmSelect.selectByVisibleText("December");
 
		WebElement dyElement = driver.findElement(By.className("react-datepicker__year-select"));
		Select dySelect = new Select(dyElement);
		dySelect.selectByValue("2000");
 
		List<WebElement> webElements2 = driver.findElements(By.xpath("//div[@aria-label='Choose Tuesday, December 26th, 2000']"));
		for(WebElement xElement : webElements2) { //
			  if(xElement.getText().equalsIgnoreCase("26")) { 
				  xElement.click(); }
			  }
		
		driver.findElement(By.id("subjectsInput")).sendKeys("Physics");
		driver.findElement(By.id("subjectsInput")).sendKeys(Keys.ENTER);
		driver.manage().timeouts().implicitlyWait(Duration.ofSeconds(5));

		//Thread.sleep(5000);
		WebElement hobby=driver.findElement(By.cssSelector("label[for='hobbies-checkbox-1']"));
		((JavascriptExecutor) driver).executeScript("arguments[0].scrollIntoView(true);", hobby);
		hobby.click();
		driver.findElement(By.id("currentAddress")).sendKeys("Navalur, Chennai");
		driver.findElement(By.id("react-select-3-input")).sendKeys("NCR");
		driver.findElement(By.id("react-select-3-input")).sendKeys(Keys.ENTER);
		driver.findElement(By.id("react-select-4-input")).sendKeys("Delhi");
		driver.findElement(By.id("react-select-4-input")).sendKeys(Keys.ENTER);
		driver.manage().timeouts().implicitlyWait(Duration.ofSeconds(2));
		driver.findElement(By.id("submit")).click();
		
	}
}
