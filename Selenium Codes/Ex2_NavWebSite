package selenium_week_1_nishchay;

import java.util.concurrent.TimeUnit;

import org.openqa.selenium.chrome.ChromeDriver;

public class Ex2_NavWebSite {

	@SuppressWarnings("deprecation")
	public static void main(String[] args) throws InterruptedException {
		// TODO Auto-generated method stub
		
		ChromeDriver driver = new ChromeDriver();
		//the above line is using 17 version of compiler
		driver.manage().window().maximize();
		driver.get("https://www.facebook.com/");
		driver.manage().timeouts().implicitlyWait(5,TimeUnit.SECONDS);
		
		System.out.println("Title of the web page is : " + driver.getTitle());
		System.out.println("1st URL of the webpage is : " + driver.getCurrentUrl());
		
		Thread.sleep(3000);
		driver.navigate().to("https://www.youtube.com/@TechieSoumyaLife/playlists");
		System.out.println("Title of the web page is : " + driver.getTitle());
		System.out.println("2nd URL of the webpage is : " + driver.getCurrentUrl());
		
		Thread.sleep(3000);
		driver.navigate().back();
		System.out.println("3rd URL of the webpage is : " + driver.getCurrentUrl());
		
		Thread.sleep(3000);
		driver.navigate().forward();
		System.out.println("4th URL of the webpage is : " + driver.getCurrentUrl());
		
		Thread.sleep(3000);
		driver.navigate().refresh();
	}

}
