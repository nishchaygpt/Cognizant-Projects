		String result17 = selectDateOfBirthYear.getFirstSelectedOption().getText();
		System.out.println("Selected Value of Date of Birth Year is : " + result17 + " (Done using ByValue)");
		
		//Date of Birth Year : By Visible Text
		selectDateOfBirthYear.selectByVisibleText("2000");
		String result18 = selectDateOfBirthYear.getFirstSelectedOption().getText();
		System.out.println("Selected Value of Date of Birth Year is : " + result18 + " (Done using ByVisibleText");
		
		//Date of Birth Year : By Send Keys
		dateOfBirthYear.click();
		dateOfBirthYear.sendKeys("2000");
		dateOfBirthYear.click();
		System.out.println("Done using sendKeys has been executed here...");
		
		Thread.sleep(3000);
		//Filling out the complete form
		
		WebElement list = driver.findElement(By.name("40cc__type"));
		Select drop = new Select(list);
		System.out.println(drop.getFirstSelectedOption().getText());
