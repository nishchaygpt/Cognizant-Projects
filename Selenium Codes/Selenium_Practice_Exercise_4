package selenium_week_1_nishchay;

import org.openqa.selenium.By;
import org.openqa.selenium.WebDriver;
import org.openqa.selenium.WebElement;
import org.openqa.selenium.chrome.ChromeDriver;
import org.openqa.selenium.support.ui.Select;

public class Selenium_Practice_Exercise_4 {

	public static void main(String[] args) throws InterruptedException {
		// TODO Auto-generated method stub

		WebDriver driver = new ChromeDriver();
		driver.manage().window().maximize();
		driver.get("https://www.roboform.com/filling-test-all-fields");
		
		WebElement creditCardType = driver.findElement(By.name("40cc__type"));
		Select selectCreditCardType = new Select(creditCardType);
		selectCreditCardType.selectByValue("0");
		String result1 = selectCreditCardType.getFirstSelectedOption().getText();
		System.out.println("Default value of Credit Card Type is : " + result1);

		
		WebElement cardExpirationMonth = driver.findElement(By.name("42ccexp_mm"));
		Select selectCardExpirationMonth = new Select(cardExpirationMonth);
		selectCardExpirationMonth.selectByValue("1");
		String result2 = selectCardExpirationMonth.getFirstSelectedOption().getText();
		System.out.println("Default value of Credit Card Expiration Month is : " + result2);
		
		WebElement cardExpirationYear = driver.findElement(By.name("43ccexp_yy"));
		Select selectCardExpirationYear = new Select(cardExpirationYear);
		String result3 = selectCardExpirationYear.getFirstSelectedOption().getText();
		System.out.println("Default value of Credit Card Expiration Year is : " + result3);

		System.out.println("\n");
		
		WebElement dateOfBirthMonth = driver.findElement(By.name("66mm"));
		Select selectDateOfBirthMonth = new Select(dateOfBirthMonth);
		String result4 = selectDateOfBirthMonth.getFirstSelectedOption().getText();
		System.out.println("Default value of Date of Birth Month is : " + result4);
		
		WebElement dateOfBirthDay = driver.findElement(By.name("67dd"));
		Select selectDateOfBirthDay = new Select(dateOfBirthDay);
		String result5 = selectDateOfBirthDay.getFirstSelectedOption().getText();
		System.out.println("Default value of Date of Birth Day is : " + result5);
		
		WebElement dateOfBirthYear = driver.findElement(By.name("68yy"));
		Select selectDateOfBirthYear = new Select(dateOfBirthYear);
		String result6 = selectDateOfBirthYear.getFirstSelectedOption().getText();
		System.out.println("Default value of Date of Birth Year is : " + result6);
		
		System.out.println("\n");
		//Credit Card Type : By Value
		selectCreditCardType.selectByValue("6");
		String result7 = selectCreditCardType.getFirstSelectedOption().getText();
		System.out.println("Selected value of Credit Card Type is : " + result7 + " (Done using ByValue)");
		
		//Credit Card Type : By Visible Text
		selectCreditCardType.selectByVisibleText("Master Card");
		String result8 = selectCreditCardType.getFirstSelectedOption().getText();
		System.out.println("Selected value of Credit Card Type is : " + result8 + " (Done using ByVisibleText)");
		
		//Credit Card Type : By Send Keys
		creditCardType.click();
		creditCardType.sendKeys("Master Card");
		creditCardType.click();
		System.out.println("Done using sendKeys has been executed here...");
		
		System.out.println("\n");
		//Credit Card Expiration Month : By Value 
		selectCardExpirationMonth.selectByValue("5");
		String result9 = selectCardExpirationMonth.getFirstSelectedOption().getText();
		System.out.println("Selected Value of Card Expiration Month is : " + result9 + " (Done using ByValue)");
		
		//Credit Card Expiration Month : By Visible Text
		selectCardExpirationMonth.selectByVisibleText("05");
		String result10 = selectCardExpirationMonth.getFirstSelectedOption().getText();
		System.out.println("Selected Value of Card Expiration Month is : " + result10 + " (Done using ByVisibleText)");
		
		//Credit Card Expiration Month : By Send Keys
		cardExpirationMonth.click();
		cardExpirationMonth.sendKeys("05");
		cardExpirationMonth.click();
		System.out.println("Done using sendKeys has been executed here...");
		
		System.out.println("\n");
		//Credit Card Expiration Year : By Value
		selectCardExpirationYear.selectByValue("2029");
		String result11 = selectCardExpirationYear.getFirstSelectedOption().getText();
		System.out.println("Selected Value of Card Expiration Year is : " + result11 + " (Done using ByValue)");
		
		//Credit Card Expiration Year : By Visible Text
		selectCardExpirationYear.selectByVisibleText("2029");
